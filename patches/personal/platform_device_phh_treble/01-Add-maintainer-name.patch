---
 board-base.mk | 2 +-
 system.prop   | 1 +
 2 files changed, 2 insertions(+), 1 deletion(-)

diff --git a/board-base.mk b/board-base.mk
index 3e62db1..63134d8 100644
--- a/board-base.mk
+++ b/board-base.mk
@@ -10,7 +10,7 @@ BOARD_ROOT_EXTRA_FOLDERS += bt_firmware sec_storage efs persist
 BUILD_BROKEN_ELF_PREBUILT_PRODUCT_COPY_FILES := true
 
 BUILD_BROKEN_DUP_RULES := true
-#BOARD_EXT4_SHARE_DUP_BLOCKS := true
+BOARD_EXT4_SHARE_DUP_BLOCKS := true
 TARGET_NO_KERNEL_OVERRIDE := true
 
 BOARD_ROOT_EXTRA_SYMLINKS := $(filter-out $(BOARD_ROOT_EXTRA_SYMLINKS),/mnt/vendor/persist:/persist)
diff --git a/system.prop b/system.prop
index db7c86d..d2bd73e 100644
--- a/system.prop
+++ b/system.prop
@@ -1,4 +1,5 @@
 ro.adb.secure=0
+ro.spark.maintainer=Nazim
 #Some devices have sdcardfs kernel panicing on 8.0
 #Disable for everyone for the moment
 ro.sys.sdcardfs=0
